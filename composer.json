{
    "name": "homeapp/filtering-repository",
    "type": "library",
    "license": "MIT",
    "description": "Filter and pagination mapper between http-request Ð¸ Doctrine ORM",
    "autoload": {
        "psr-4": {
            "Homeapp\\Filter\\": "src/Filter/",
            "Homeapp\\FilteringRepository\\": "src/FilteringRepository/"
        }
    },
    "autoload-dev": {
